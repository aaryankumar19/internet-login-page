{
    "action": {
        "default_popup": "popupmain.html"
    },
    "content_scripts": [
        {
            "js": [ "autologin.js" ],
            "matches": [
            "https://internet.lpu.in/*",
            "*://*/24online/webpages/client.jsp*",
            "*://*/24online/webpages/waitrequest.jsp*",
            "*://*/24online/servlet/E24onlineHTTPClient*"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
          "resources": ["images/nazi-logo.png","images/hitler.jpg"],
          "matches": ["<all_urls>"]
        }
    ],
    "description": "To login automatically in WIFI-Internet",
    "manifest_version": 3,
    "name": "Internet-login",
    "permissions": [ "activeTab", "storage", "tabs"],
    "version": "1.0.0"
}
